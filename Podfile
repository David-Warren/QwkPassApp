  pod 'Firebase/Core'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'
  pod 'Firebase/Database'
  pod 'FirebaseUI', '~> 4.0'
  use_frameworks!

  # Pods for QwkPassApp
  
  target 'QwkPassAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'QwkPassAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end
